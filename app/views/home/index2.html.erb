<h1>Home#index</h1>
<p>Find me in app/views/home/index2.html.erb I`m the functionary view</p>
<div style="float:left">
  There are <%= Question.count(:conditions=>"questions.id NOT IN (SELECT DISTINCT question_id FROM answers)")%> unanswered questions
  <table>
    <tr>
      <th>
        Subject
      </th>
      <th>
        Asked by
      </th>
      <th>
        Date
      </th>
      <th>
      </th>
      <th>
      </th>
    </tr>
    <%  @lastten.each do |x| %>
      <tr>
        <td>
          <%= x.subject %>
        </td>
        <td>
          <%= x.user.email %>
        </td>
        <td>
          <%= x.created_at %>
        </td>
        <td>
          <% if x.dialogue %>
            Dialogue
          <% end %>
        </td>
        <td>
          <%= link_to 'Show', x %>
        </td>
      </tr>
    <% end %>
  </table>
</div>
<div style="float:right">RIGHT</div>
