<%= render 'menu' %>

<%if current_user.has_role?(:admin)%>
  <fieldset>
    <legend>Functionaries</legend>
    <table class="table table-bordered table-striped">
      <thead>
        <th>Navn</th>
        <th>#5</th>
        <th>#6</th>
        <th>#8</th>
      </thead>
      <% @functionaries.each do |f| %>
        <tr>
          <td><%= f.first_name + " " + f.last_name %></td>
          <% d5 = @deadline_five.select{|d| d.functionary_id == f.id}.first %>
          <td><%= d5.attributes["count"] if d5 %></td>
          <% d6 = @deadline_six.select{|d| d.functionary_id == f.id}.first %>
          <td><%= d6.attributes["count"] if d6 %></td>
          <% d8 = @deadline_eight.select{|d| d.functionary_id == f.id}.first %>
          <td><%= d8.attributes["count"] if d8 %></td>
        </tr>
      <% end %>
    </table>
  </fieldset>
<%end%>